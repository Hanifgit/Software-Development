package Lab_05;

import java.util.Scanner;

public class TestProgram {
	public static void main(String[] args) {
		PersonalInfo objInfo;
		Student objStudent = new Student();
		
		Scanner input = new Scanner(System.in);
		
		objStudent.setId(input.nextInt());
		objStudent.setHome_district(input.nextLine());
		objStudent.setHeight(input.nextInt());
		objStudent.setGender(input.nextLine());
		objStudent.setName(input.nextLine());
		objStudent.setDept(input.nextLine());
		objStudent.setCgpa(input.nextDouble());
	}
}
