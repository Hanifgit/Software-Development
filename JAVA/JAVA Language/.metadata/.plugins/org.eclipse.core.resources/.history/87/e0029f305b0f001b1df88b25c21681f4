package individualPresentation;

public class ThreadMethod_01 {
	public static void main(String[] args) {
		U objA = new U();
		V objB = new V();

		objA.my();
		objB.my();
	}
}

class U {
	public void my() {
		for (int i = 0; i <= 10; i++) {
			System.out.println("R");
		}
	}
}

class V {
	public void my() {
		for (int i = 0; i <= 10; i++)
			System.out.println("A");
	}
}
